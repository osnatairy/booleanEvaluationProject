extends layout

block content
  h1 Boolean Evaluation List
  - var i = 1;

  table.table
    thead
      tr 
        th(scope='col') #num      
        th(scope='col') id
        th(scope='col') folder
        th(scope='col') conceptCount
        th(scope='col') Terms
        th(scope='col') Clauses
        th(scope='col') Expressions
        th(scope='col') rounds
        th(scope='col') Method
        th(scope='col') ValueOrder 
        th(scope='col') TrueEvaluate
        th(scope='col') FalseEvaluate
        th(scope='col') avgRunTime
        th(scope='col')
        
    tbody
      each element in docs
        tr
          td #{i++}
          td #{element.evaluationid}   
          td #{element.folder} 
            //== undefined ? "" : element.folder.split('/')[element.folder.split('/').length - 2]}   
          td #{element.conceptList != undefined ? Object.keys(JSON.parse(element.conceptList)).length : 0} 
          td #{element.sumTerms}
          td #{element.sumClauses}
          td #{element.sumExpressions} 
          td #{element.round}
          td #{element.algorithm} #{element.probMethod} 
          td #{element.probValueOrder}
          td #{element.sumTrueEvaluate}
          td #{element.sumFalseEvaluate}
          td #{element.avgRunTime}
          
          td(scope='col') 
            | 
            a(href="data/probsdata?id=#{element.evaluationid}&sumExpressions=#{element.sumExpressions}") 
              //img(src="images/tables.png", alt="probes") 
              svg.bi.bi-table(width='1em', height='1em', viewBox='0 0 16 16', fill='currentColor', xmlns='http://www.w3.org/2000/svg')
                path(fill-rule='evenodd', d='M0 2a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v12a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V2zm15 2h-4v3h4V4zm0 4h-4v3h4V8zm0 4h-4v3h3a1 1 0 0 0 1-1v-2zm-5 3v-3H6v3h4zm-5 0v-3H1v2a1 1 0 0 0 1 1h3zm-4-4h4V8H1v3zm0-4h4V4H1v3zm5-3v3h4V4H6zm4 4H6v3h4V8z')
            &nbsp;
            &nbsp;
            a(href="data/barchart?id=#{element.evaluationid}") 
              //img(src="images/tables.png", alt="probes") 
              svg.bi.bi-bar-chart-line-fill(width='1em', height='1em', viewBox='0 0 16 16', fill='currentColor', xmlns='http://www.w3.org/2000/svg')
                rect(width='4', height='5', x='1', y='10', rx='1')
                rect(width='4', height='9', x='6', y='6', rx='1')
                rect(width='4', height='14', x='11', y='1', rx='1')
                path(fill-rule='evenodd', d='M0 14.5a.5.5 0 0 1 .5-.5h15a.5.5 0 0 1 0 1H.5a.5.5 0 0 1-.5-.5z')
            &nbsp;
            &nbsp;
            a(href="data/graph?id=#{element.evaluationid}")
              svg.bi.bi-graph-up(width='1em', height='1em', viewBox='0 0 16 16', fill='currentColor', xmlns='http://www.w3.org/2000/svg')
                path(d='M0 0h1v16H0V0zm1 15h15v1H1v-1z')
                path(fill-rule='evenodd', d='M14.39 4.312L10.041 9.75 7 6.707l-3.646 3.647-.708-.708L7 5.293 9.959 8.25l3.65-4.563.781.624z')
                path(fill-rule='evenodd', d='M10 3.5a.5.5 0 0 1 .5-.5h4a.5.5 0 0 1 .5.5v4a.5.5 0 0 1-1 0V4h-3.5a.5.5 0 0 1-.5-.5z')
                  


            


  